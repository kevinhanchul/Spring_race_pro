<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.FepInsertMapper">
    <select id="selFepInsert" parameterType="map" resultType="map">
        select
			USER_NAME,
			USER_ID,
			USER_PW,
			USER_EMAIL
		from t_fep_info
		LIMIT 0,1
    </select>
</mapper>
