<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.HealthInfoMapper">
    <select id="selHealthInfo" parameterType="long" resultType="map">
        select 
			user_name,
			race_no,
			win_type,
			runner_no,
			bet_amt,
			win_rate,
			ret_amt
		from t_healthInfo
		LIMIT 0,1
    </select>
</mapper>
